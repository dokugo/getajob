{
  "processors": [
    "stylelint-processor-styled-components"
  ],
  "extends": [
    "stylelint-config-recommended",
    "stylelint-config-styled-components",
    "stylelint-config-prettier"
  ],
  "plugins": [
    "stylelint-prettier"
  ],
  "rules": {
    "prettier/prettier": true,
    "rule-empty-line-before": [
      "always",
      {
        "except": [
          "inside-block",
          "first-nested"
        ]
      }
    ]
  }
}